<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
        "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
    <constant name="struts.devMode" value="true"/>
    <constant name="struts.enable.DynamicMethodInvocation" value="true"/>

    <package name="cart" extends="struts-default" namespace="/cart">
        <action name="addCart" class="com.mlj.dangdang.action.CartAction" method="addCart">
            <result name="cart">
               /cart/cart_list.jsp
            </result>
        </action>
        <action name="update" class="com.mlj.dangdang.action.CartAction" method="update">
            <result name="cart">
                /cart/cart_list.jsp
            </result>
        </action>
    </package>

</struts>